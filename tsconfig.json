{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@constants/*": ["constants/*"],
      "@navigation/*": ["navigation/*"],
      "@/*": ["*"],
      "@styles/*": ["styles/*"],
      "@redux/*": ["redux/*"],
    }
  }
}
